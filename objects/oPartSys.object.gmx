            <kind>1</kind>
            <string>///Init Particle System

// Have different systems for different layers
global.pSysMain = part_system_create();
global.pSysAbove = part_system_create();
global.pSysBelow = part_system_create();
global.pSysBackground = part_system_create();

part_system_depth(global.pSysMain, 0);
part_system_depth(global.pSysAbove, -1);
part_system_depth(global.pSysBelow, 1);
part_system_depth(global.pSysBackground, 999);

// FX Part
scrInitPartShipTrail();
scrInitPartShipEnemyTrail();
scrInitPartBulletFire();
scrInitShipShrapnel();
scrInitPartDeathCurrency();

scrInitPartAmbientSmokePink(0);
scrInitPartAmbientSmokeOrange(0);
scrInitPartAmbientTwinklingStars(0);
scrInitPartAmbientSmokeBlue(0);

// Conditionals
if (room == rmArenaBasicExt) {
    scrInitPartAmbientSmokePink(1);
    scrInitPartAmbientSmokeOrange(1);
    scrInitPartAmbientTwinklingStars(1);
} else if (room == rmArenaBasic) {
    scrInitPartAmbientSmokePink(1);
    scrInitPartAmbientTwinklingStars(1);
} else if (room == rmArenaMinimal) {
    scrInitPartAmbientSmokePink(1);
    scrInitPartAmbientTwinklingStars(1);
} else if (room == rmArenaDoublePass) {
    scrInitPartAmbientSmokeOrange(1);
    scrInitPartAmbientTwinklingStars(2);
} else if (room == rmArenaDuo) {
    scrInitPartAmbientSmokePink(1);
    scrInitPartAmbientTwinklingStars(1);
} else if (room == rmArenaQuadFlank) {
    scrInitPartAmbientSmokeBlue(2);
    scrInitPartAmbientSmokeOrange(1);
    scrInitPartAmbientTwinklingStars(2);
} 

if (room == rmOverworldMenu) {
    scrInitPartAmbientSmokePink(1);
}
</string>
